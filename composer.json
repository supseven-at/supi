{
    "name": "supseven/supi",
    "description": "Cookie Consent popover; easy, yet powerful",
    "license": "GPL-3.0-or-later",
    "type": "typo3-cms-extension",
    "autoload": {
        "psr-4": {
            "Supseven\\Supi\\": "Classes/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Supseven\\Supi\\Tests\\": "Tests/"
        }
    },
    "require": {
        "php": "^8.5",
        "typo3/cms-backend": "^14.1",
        "typo3/cms-core": "^14.1",
        "typo3/cms-extbase": "^14.1",
        "typo3/cms-fluid": "^14.1",
        "typo3/cms-frontend": "^14.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^12",
        "friendsofphp/php-cs-fixer": "^3.0",
        "mikey179/vfsstream": "^1.6"
    },
    "extra": {
        "typo3/cms": {
            "extension-key": "supi"
        }
    },
    "config": {
        "allow-plugins": {
            "typo3/class-alias-loader": false,
            "typo3/cms-composer-installers": false
        }
    }
}
